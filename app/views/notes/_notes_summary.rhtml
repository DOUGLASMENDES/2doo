<% note = notes_summary -%>
<div class="note_wrapper" id="<%= dom_id(note) %>">
<%= link_to( 
  image_tag("blank.png", :border => 0),
  note,
  :title => t('notes.show_note_title'),
  :class => "link_to_notes icon",
  :id => dom_id(note, "link")) %>&nbsp;
<%= rendered_note(note) %>
</div>
<% note = nil -%>
